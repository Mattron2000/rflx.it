<link rel="stylesheet" href="/css/home.css" />

<h1>Welcome to the Home Page</h1>

<br />

<div id="dati-utente"></div>

<hr />

<form action="/api/v1/photos" method="post" enctype="multipart/form-data" id="photo-uploader">
	<input type="file" name="photo" accept=".jpg,.jpeg,.png" />
	<button type="submit">Upload</button>
</form>

<hr />

<section class="feed" id="photo-feed">
</section>

<div
	class="modal fade"
	id="uploadFeedback"
	tabindex="-1"
	aria-labelledby="uploadFeedbackLabel"
	aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
								<button
					id="uploadFeedbackDismiss"
					type="button"
					class="btn-close"
					data-bs-dismiss="modal"
					aria-label="Close"></button>
			</div>
			<div class="modal-body" id="uploadFeedbackLabel"></div>
					</div>
	</div>
</div>
