root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = tab
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 80

[*.md]
indent_style = space

## Non-configurable Prettier behaviors
# charset
# insert_final_newline
# trim_trailing_whitespace

## Configurable Prettier behaviors
# end_of_line
# indent_style
# indent_size
# max_line_length
